{
	"name": "vahrplan",
	"version": "5.0.0 beta 3",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node --env-file=.env build/index.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "svelte-kit sync && eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:fix": "svelte-kit sync && npm run lint -- --fix",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"test": "vitest run"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/eslint": "^9.6.1",
		"@types/hafas-client": "^6.2.0",
		"@types/leaflet": "^1.9.15",
		"@types/node": "^25.0.2",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.16.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"jsdom": "^27.3.0",
		"leaflet": "^1.9.4",
		"prettier": "^3.5.0",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.37.2",
		"svelte-check": "^4.2.1",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.35.1",
		"vite": "^7.3.0",
		"vitest": "^4.0.15"
	},
	"type": "module",
	"dependencies": {
		"db-vendo-client": "^6.10.4",
		"hafas-client": "^6.3.6",
		"leaflet": "^1.9.4",
		"redis": "^5.10.0"
	}
}
